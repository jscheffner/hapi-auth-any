{
  "name": "hapi-auth-any",
  "version": "1.0.0",
  "description": "hapi auth scheme, combines multiple other strategies and passes if any of them does",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava",
    "test:hapi19": "cross-env HAPI_VERSION=19 nyc ava",
    "test:hapi20": "cross-env HAPI_VERSION=20 nyc ava",
    "test:hapi21": "cross-env HAPI_VERSION=21 nyc ava",
    "lint": "eslint test index.js test/**/*.js examples/**/*.js",
    "format": "npm run lint -- --fix"
  },
  "files": [
    "index.js"
  ],
  "author": "Jonas Scheffner",
  "repository": "github:jscheffner/hapi-auth-any",
  "keywords": [
    "hapi",
    "hapijs",
    "hapi.js",
    "auth",
    "authentication",
    "access",
    "credentials",
    "scope",
    "any",
    "plugin",
    "strategy",
    "strategies",
    "combine",
    "scheme",
    "schema"
  ],
  "license": "ISC",
  "devDependencies": {
    "ava": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "hapi21": "npm:@hapi/hapi@^21.3.0",
    "nyc": "^15.0.0"
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "p-any": "^3.0.0"
  },
  "peerDependencies": {
    "@hapi/hapi": "21.x"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
